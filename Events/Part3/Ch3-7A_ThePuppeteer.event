SluiceGateFlood:
	CameraCursor(12,5)
	EARTHQUAKE_START 0x100
	STAL 60
	CHECK_FLAG Local_BossDeath
	BNE 0x666 rC r0
		MapText(Ch3_7A_Flood)
	LABEL 0x666
	FADEIN 2
	STAL 1
	EARTHQUAKE_END
	SetBackground(BlackBackground)
	GameOver
	TERMINATE

