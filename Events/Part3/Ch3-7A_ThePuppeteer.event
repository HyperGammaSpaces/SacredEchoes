Ch3_7A_BeginningScene:
	MUSIC SongID_FromTheDarkness
	SVAL 0x2 (EnemyUnits|IsPointer)
	CHECK_ISHARD
	BEQ 0x3A07 rC r0
		SVAL 0x2 (EnemyUnits_Hard|IsPointer)
	LABEL 0x3A07
	LOAD_S2 0x1
	ENUN
	FADEOUT 5

	SVAL 0x2 (CutsceneUnits_NoLuthier|IsPointer)
	SVAL 0xA 0x0
	CHECK_EXISTS Luthier
	BEQ 0x3A17 rC r0
		SVAL 0x2 (CutsceneUnits|IsPointer)
	LABEL 0x3A17
	LOAD_CUTSCENE_S2
	ENUN
	CAMERA_CENTERED [12, 5]
	STAL 30
	CameraCursor(Alm)
	MapText(Ch3_7A_OpeningText)

	SVAL 0x2 Tatarrah
	_WARP 0xFFFF 0xFFFD [9,17]
	CALL Warp_In_Effect
	STAL 20
	SVAL 0x2 DeltheaBoss
	_WARP 0xFFFF 0xFFFD [8,17]
	CALL Warp_In_Effect
	STAL 20

	CameraCursor(Tatarrah)
	MapText(Ch3_7A_OpeningText_2)

	SVAL 0x2 Tatarrah
	CALL Warp_Out_Effect
	STAL 0x08
	REMOVEUNIT Tatarrah

	CAMERA_CENTERED DeltheaBoss
	EVBIT_T 0x9
	MOVE 0x08 DeltheaBoss [10, 18]
	ENUN
	EVBIT_F 0x9

	StartBattle
	SetBattleNumbers(78,19,4,0xFF,0xFF,0xFF)
	MissedAttack(0,0)
	EndAttack
	FIGHT DeltheaBoss Alm Aura 0

	CHECK_EXISTS Luthier
	BNE 0x3 0xC 0xA

		MapText(Ch3_7A_OpeningText_LuthierExists)
		CHECK_ISALIVE Luthier
		BEQ 0x4 0xC 0x0
			CameraCursor(Luthier)
			MapText(Ch3_7A_OpeningText_LuthierAlive)
			GOTO 0x4
	LABEL 0x3
		MapText(Ch3_7A_OpeningText_NoLuthier)
	LABEL 0x4
	STAL 20
	SVAL 0x2 DeltheaBoss
	CALL Warp_Out_Effect
	STAL 0x08
	REMOVEUNIT DeltheaBoss

	FADEIN 5
	REMOVEALL_BLUE
	REMOVEALL_RED
	LOAD 0x1 EnemyUnits
	ENUN
	LOAD_DEPLOYED 0x0 Ch3_7A_Units1
	ENUN
	GotoPrepScreen
	END

DeltheaWakesUp:
	CHECK_ISALIVE DeltheaBoss
	BEQ 0x2 0xC 0x0
		FLAG_T DeltheaLocalFlag
		MUSIC_QUIET_T
		CameraCursor(DeltheaBoss)
		MapText(Ch3_7A_DeltheaWakesUp)
		NoFade
		STAL 20
		FACTION_GREEN DeltheaBoss
		STAL 20
		SVAL 0x2 DeltheaBoss
		CALL Warp_Out_Effect
		STAL 0x08
		REMOVEUNIT DeltheaBoss
		MUSIC_QUIET_F
	LABEL 0x2
	NoFade
	END

Ch3_7A_EndingScene:
	CONFIG_SKIP 0x1
	FLAG_T gChapter3A7Completed
	CONFIG_SKIP 0x0
	CALL EndMusicAndMMS
	CALL UnifiedBEXPCaller
	FADEIN 2
	STAL 5
	SetBackground(Castle_Hall_Night)
	CHECK_ISALIVE Luthier
	BEQ 0x4 0xC 0x0
		CHECK_FLAG DeltheaLocalFlag
		BEQ 0x5 0xC 0x0
			MUSIC SongID_Unity
			TEXTMODE 1
			TEXTSHOW Ch3_7A_EndingText //both alive
			TEXTEND
			FADEIN 2
			STAL 20
			FADEOUT 2
			TEXTCONT
			TEXTEND
			MUSIC SongID_CastleTheme
			TEXTCONT
			TEXTEND
			TEXTCLEAR
			FADEIN 2
			CLEAN
			CONFIG_SKIP 0x1
			LOAD 0x1 DeltheaJoins
			ENUN
			SetBackground(BlackBackground,5)
			CALL $591F40
			CharacterJoinedNotification(Notif_DeltheaJoin)
			CONFIG_SKIP 0x0
			FADEIN 5
			GOTO 0x6
		LABEL 0x5
			MUSIC SongID_SilentGround
			BackgroundText(Ch3_7A_Ending_LuthierOnly)
			FADEIN 2
			GOTO 0x6
	LABEL 0x4 //luthier dead
	CHECK_FLAG DeltheaLocalFlag
		BEQ 0x6 0xC 0x0
		MUSIC SongID_Recruitment
		TEXTMODE 1
		TEXTSHOW Ch3_7A_Ending_DeltheaOnly //only delthea
		TEXTEND
		FADEIN 2
		STAL 20
		FADEOUT 2
		TEXTCONT
		TEXTEND
		MUSIC SongID_CastleTheme
		TEXTCONT
		TEXTEND
		TEXTCLEAR
		FADEIN 2
		CLEAN
		CONFIG_SKIP 0x1
		LOAD 0x1 DeltheaJoins
		ENUN
		SetBackground(BlackBackground,5)
		CALL $591F40
		CharacterJoinedNotification(Notif_DeltheaJoin)
		CONFIG_SKIP 0x0
		FADEIN 5
	LABEL 0x6
	SetBackground(BlackBackground)
	CONFIG_SKIP 0x1
	CHECK_FLAG gChapter3C7Completed
	BEQ 0x13 0xC 0x0
		//If gChapter3A7Completed
		CALL Ch3_EndingScene
	//else
	LABEL 0x13
	CONFIG_SKIP 0x0
	SetBackground(Castle_Hall_Night,5)
	BackgroundText(Ch3_EndingText_SluiceGate_NotOpen)
	FADEIN 2

	GOTO_CHAPTER 0x1E
	SHORT $0D40 $0
	POIN Act3EndSwitch|1
	END
